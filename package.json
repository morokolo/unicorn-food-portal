{
    "name": "unicorn-food-portal",
    "version": "0.0.1",
    "description": "Angular 1 that is ready for Angular 2 ",
    "main": "./src/index.js",
    "scripts": {
      "lint": "eslint ./src --ext .js",
      "clean-dist": "rimraf dist",
      "precommit": "npm run testci && npm run lint",
      "copy-files": "cpy src/index.html src/favicon.ico dist",
      "clean-and-copy": "npm run clean-dist && npm run copy-files",
      "prestart": "npm run clean-and-copy",
      "start": "env-cmd ./build/.env.local webpack-dev-server --config ./build/webpack.config.js --inline --open --history-api-fallback --content-base dist",
      "prebuild": "npm run clean-and-copy",
      "build": "env-cmd ./build/.env.local webpack --config ./build/webpack.config.js",
      "prebuild:docker": "npm run clean-and-copy",
      "prebuild:prod": "npm run clean-and-copy",
      "build:docker": "webpack --config ./build/webpack.config.js",
      "docker": "npm run build:docker && docker-compose stop && docker-compose up",
      "docker:d": "npm run build:docker && docker-compose stop && docker-compose up -d",
      "build:prod": "set NODE_ENV=dev && webpack",
      "test": "env-cmd ./build/.env.local karma start test/karma.conf.js",
      "testci": "env-cmd ./build/.env.ci karma start test/karma.conf.js",
      "testlocal": "env-cmd ./build/.env.local karma start test/karma.conf.local.js"
    },
    "keywords": [
      "AngularJS",
      "ES6",
      "Webpack"
    ],
    "author": "Kwakhona Mahamba <kwakhona@tangentsolutions.co.za>",
    "devDependencies": {
      "angular-mocks": "^1.5.8",
      "babel-core": "^6.14.0",
      "babel-eslint": "^7.2.3",
      "babel-loader": "^6.2.5",
      "babel-plugin-__coverage__": "^11.0.0",
      "babel-preset-es2015": "^6.14.0",
      "babel-preset-stage-2": "^6.13.0",
      "copy-webpack-plugin": "^4.0.1",
      "cpy-cli": "^1.0.1",
      "css-loader": "^0.25.0",
      "eslint": "^3.19.0",
      "file-loader": "^0.9.0",
      "html-webpack-plugin": "^2.29.0",
      "husky": "^0.13.3",
      "jasmine-core": "^2.5.2",
      "karma": "^1.1.0",
      "karma-chrome-launcher": "^2.0.0",
      "karma-coverage": "^1.1.1",
      "karma-jasmine": "^1.0.2",
      "karma-phantomjs-launcher": "^1.0.2",
      "karma-spec-reporter": "0.0.24",
      "karma-webpack": "^1.8.0",
      "ng-annotate-loader": "^0.1.1",
      "node-sass": "^3.10.0",
      "phantomjs-prebuilt": "^2.1.12",
      "raw-loader": "^0.5.1",
      "rimraf": "^2.5.4",
      "sass-loader": "^4.0.2",
      "style-loader": "^0.13.1",
      "url-loader": "^0.5.7",
      "webpack": "^1.13.2",
      "webpack-css-loaders": "^1.0.0",
      "webpack-dev-server": "^1.16.1"
    },
    "dependencies": {
      "angular": "^1.5.8",
      "angular-animate": "^1.5.8",
      "angular-aria": "^1.5.8",
      "angular-cookies": "^1.5.8",
      "angular-sanitize": "^1.5.8",
      "angular-toastr": "^2.1.1",
      "angular-touch": "^1.5.8",
      "angular-ui-bootstrap": "^2.1.3",
      "@uirouter/angularjs": "^1.0.0-beta.2",
      "bootstrap": "^3.3.7",
      "copy-webpack-plugin": "^4.0.1",
      "env-cmd": "^5.1.0",
      "font-awesome": "^4.6.3",
      "highcharts": "^5.0.11",
      "html-loader": "^0.4.5",
      "i": "^0.3.5",
      "jquery": "^3.1.0",
      "mobx": "^2.5.1",
      "moment": "^2.18.1",
      "rx-angular": "^1.1.3",
      "save-dev": "^2.0.0",
      "toastr": "^2.1.2",
      "underscore": "^1.8.3"
    }
  }
  